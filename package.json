{
  "name": "objectified",
  "version": "0.2.1",
  "main": "index.js",
  "description": "One-way DOM-to-model data-binding.",
  "homepage": "https://github.com/cfpb/objectified",
  "bugs": "https://github.com/cfpb/objectified/issues",
  "author": {
    "name": "Consumer Financial Protection Bureau",
    "url": "http://consumerfinance.gov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/objectified"
  },
  "license": "Public Domain",
  "keywords": [
    "objectified",
    "browserify",
    "data-binding",
    "form"
  ],
  "devDependencies": {
    "nodeunit": "~0.8.6",
    "browserify": "^4.1.3",
    "casperjs": "~1.1.0-beta3",
    "uglify-js": "~2.4.15"
  },
  "scripts": {
    "test": "./node_modules/.bin/browserify index.js -s objectify -o test/objectified.js && ./node_modules/.bin/casperjs test test/objectified-tests.js",
    "browser": "mkdir -p dist && ./node_modules/.bin/browserify index.js -s objectify -o dist/objectified.js && ./node_modules/.bin/uglifyjs dist/objectified.js -m -c sequences,dead_code,conditionals,booleans,unused,if_return,join_vars,drop_console -o dist/objectified.min.js"
  },
  "dependencies": {
    "debounce": "~1.0.0",
    "unformat-usd": "~0.1.1"
  }
}
